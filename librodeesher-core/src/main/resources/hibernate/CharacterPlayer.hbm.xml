<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd"> 
 
<hibernate-mapping>
    <class name="com.softwaremagico.librodeesher.pj.CharacterPlayer" table="CharacterPlayer" catalog="LibroDeEsher">
        <id name="id" type="java.lang.Long">
            <column name="ID" />
            <generator class="native" />
        </id>
	 <property name="name" type="string">
            <column name="NAME" />
        </property>
        <property name="raceName" type="string">
            <column name="RACE" />
        </property>
        <property name="cultureName" type="string">
            <column name="CULTURE" />
        </property>
    </class>
</hibernate-mapping>