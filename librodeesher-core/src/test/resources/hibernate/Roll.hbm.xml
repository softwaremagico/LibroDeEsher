<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="com.softwaremagico.librodeesher.basics"> 
    <class name="Roll" table="T_ROLL">
        <id name="id" column="Id">
            <generator class="native"/>
        </id>
       <property name="firstDiceResult" column="firstDiceResult" />
       <property name="secondDiceResult" column="secondDiceResult" />
    </class>
 
</hibernate-mapping>